43d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u044b \u0441\u043b\u0435\u0434\u0443\u044e\u0449\u0438\u0435 \u0443\u0442\u043e\u0447\u043d\u0435\u043d\u0438\u044f:\r\n1.  \u0412\u043d\u0443\u0442\u0440\u0435\u043d\u043d\u0438\u0435 \u0433\u0430\u0431\u0430\u0440\u0438\u0442\u044b \u043a\u0430\u043c\u0435\u0440\u044b (\u0434\u043b\u0438\u043d\u043d\u0430 \u0445 \u0448\u0438\u0440\u0438\u043d\u0430 \u0445 \u0432\u044b\u0441\u043e\u0442\u0430, \u043c\u043c) \u2013\r\n2. \u0422\u043e\u043b\u0449\u0438\u043d\u0430 \u0442\u0435\u043f\u043b\u043e\u0438\u0437\u043e\u043b\u044f\u0446\u0438\u0438 \u0441\u0442\u0435\u043d, \u043f\u043e\u043b\u0430, \u043f\u043e\u0442\u043e\u043b\u043a\u0430, \u0434\u0432\u0435\u0440\u0438, \u043c\u043c \u2013\r\n3. \u041c\u0430\u0442\u0435\u0440\u0438\u0430\u043b \u0442\u0435\u043f\u043b\u043e\u0438\u0437\u043e\u043b\u044f\u0446\u0438\u0438 \u0441\u0442\u0435\u043d, \u043f\u043e\u043b\u0430, \u043f\u043e\u0442\u043e\u043b\u043a\u0430, \u0434\u0432\u0435\u0440\u0438 \u2013\r\n4. \u0420\u0430\u0437\u043c\u0435\u0440 \u0441\u0432\u0435\u0442\u043e\u0432\u043e\u0433\u043e \u043f\u0440\u043e\u0435\u043c\u0430 \u0434\u0432\u0435\u0440\u0438 (\u0448\u0438\u0440\u0438\u043d\u0430 \u0445 \u0432\u044b\u0441\u043e\u0442\u0430, \u043c\u043c)\r\n5. \u041e\u0440\u0438\u0435\u043d\u0442\u0438\u0440\u043e\u0432\u043e\u0447\u043d\u043e\u0435 \u043a\u043e\u043b-\u0432\u043e \u0437\u0430\u0445\u043e\u0434\u043e\u0432 \u043f\u0435\u0440\u0441\u043e\u043d\u0430\u043b\u0430 \u0432 \u043a\u0430\u043c\u0435\u0440\u0443 \u0438 \u0432\u0440\u0435\u043c\u044f \u0438\u0445 \u043d\u0430\u0445\u043e\u0436\u0434\u0435\u043d\u0438\u044f \u0432 \u043a\u0430\u043c\u0435\u0440\u0435 (\u0432 \u0442\u0435\u0447\u0435\u043d\u0438\u0438 \u0441\u0443\u0442\u043e\u043a) \u2013\r\n6. \u0415\u0441\u0442\u044c \u043b\u0438 \u0441\u0438\u043b\u0438\u043a\u043e\u043d\u043e\u0432\u0430\u044f \u0448\u0442\u043e\u0440\u043a\u0430 \u043d\u0430 \u0434\u0432\u0435\u0440\u044f\u0445 -\r\n7. \u0422\u0435\u043c\u043f\u0435\u0440\u0430\u0442\u0443\u0440\u043d\u044b\u0439 \u0440\u0435\u0436\u0438\u043c \u0432 \u043a\u0430\u043c\u0435\u0440\u0435, \u00b0\u0421\r\n8. \u0422\u0435\u043c\u043f\u0435\u0440\u0430\u0442\u0443\u0440\u0430 \u0437\u0430\u0433\u0440\u0443\u0436\u0430\u0435\u043c\u043e\u0433\u043e \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u0430, \u00b0\u0421\r\n9. \u041a\u043e\u043d\u0435\u0447\u043d\u0430\u044f \u0442\u0435\u043c\u043f\u0435\u0440\u0430\u0442\u0443\u0440\u0430 \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u0430, \u00b0\u0421\r\n10. \u0420\u0435\u0436\u0438\u043c \u0440\u0430\u0431\u043e\u0442\u044b \u043a\u0430\u043c\u0435\u0440\u044b (\u0445\u0440\u0430\u043d\u0435\u043d\u0438\u0435, \u043e\u0445\u043b\u0430\u0436\u0434\u0435\u043d\u0438\u0435, \u0437\u0430\u043c\u043e\u0440\u043e\u0437\u043a\u0430).\r\n11. \u0422\u0435\u043c\u043f\u0435\u0440\u0430\u0442\u0443\u0440\u0430 \u0441\u043d\u0430\u0440\u0443\u0436\u0438 \u043a\u0430\u043c\u0435\u0440\u044b.\r\n12. \u041a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u044f \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u0430 (\u043c\u044f\u0441\u043e, \u0440\u044b\u0431\u0430, \u0436\u0438\u0440\u044b, \u0433\u0430\u0441\u0442\u0440\u043e\u043d\u043e\u043c\u0438\u044f, \u043e\u0432\u043e\u0449\u0438, \u0444\u0440\u0443\u043a\u0442\u044b \u0438 \u0442\u0434).\r\n13. \u0425\u0440\u0430\u043d\u0438\u043c\u0430\u044f \u043c\u0430\u0441\u0441\u0430, \u043a\u0433 \u2013\r\n14. \u0421\u0443\u0442\u043e\u0447\u043d\u044b\u0439 \u043e\u0431\u043e\u0440\u043e\u0442, \u043a\u0433 \u2013\r\n15. \u0412\u0438\u0434 \u0442\u0430\u0440\u044b, \u0443\u043f\u0430\u043a\u043e\u0432\u043a\u0438 \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u0430 (\u043a\u0430\u0440\u0442\u043e\u043d, \u043f\u043b\u0430\u0441\u0442\u0438\u043a, \u0431\u0435\u0437 \u0443\u043f\u0430\u043a\u043e\u0432\u043a\u0438) \u2013\r\n16. \u0422\u0438\u043f \u043e\u0441\u0432\u0435\u0449\u0435\u043d\u0438\u044f \u0432 \u043a\u0430\u043c\u0435\u0440\u0435 (\u043b\u0430\u043c\u043f\u044b \u043d\u0430\u043a\u0430\u043b\u0438\u0432\u0430\u043d\u0438\u044f, \u043c\u043e\u0449\u043d\u043e\u0441\u0442\u044c \u0438 \u0442\u0434) \u0438 \u0432\u0440\u0435\u043c\u044f \u0435\u0433\u043e \u0440\u0430\u0431\u043e\u0442\u044b \u0447\u0430\u0441\u043e\u0432/\u0441\u0443\u0442\u043a\u0438.\r\n17. \u041c\u0435\u0441\u0442\u043e\u0440\u0430\u0441\u043f\u043e\u043b\u043e\u0436\u0435\u043d\u0438\u0435 \u0430\u0433\u0440\u0435\u0433\u0430\u0442\u043e\u0432 (\u0440\u0430\u0441\u0441\u0442\u043e\u044f\u043d\u0438\u0435 \u043e\u0442 \u0430\u0433\u0440\u0435\u0433\u0430\u0442\u043e\u0432 \u0434\u043e \u043a\u0430\u043c\u0435\u0440).\r\n18. \u041d\u0430\u043b\u0438\u0447\u0438\u0435 \u0432\u0435\u043d\u0442\u0438\u043b\u044f\u0446\u0438\u0438 \u0432 \u043f\u043e\u043c\u0435\u0449\u0435\u043d\u0438\u0438 \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043a\u0438 \u0430\u0433\u0440\u0435\u0433\u0430\u0442\u043e\u0432.\r\n19. \u041e\u0431\u0435\u0441\u043f\u0435\u0447\u0438\u0432\u0430\u0435\u0442\u0441\u044f \u043b\u0438 \u043f\u043e\u0434\u0430\u0447\u0430 \u043c\u043e\u0440\u0441\u043a\u043e\u0439, \u043e\u0442\u0444\u0438\u043b\u044c\u0442\u0440\u043e\u0432\u0430\u043d\u043d\u043e\u0439 \u0432\u043e\u0434\u044b \u043a \u043c\u0435\u0441\u0442\u0443 \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043a\u0438 \u0430\u0433\u0440\u0435\u0433\u0430\u0442\u043e\u0432.\r\n20. \u0422\u0435\u043c\u043f\u0435\u0440\u0430\u0442\u0443\u0440\u0430 \u0432\u043e\u0434\u044b \u043f\u043e\u0434\u0432\u043e\u0434\u0438\u043c\u043e\u0439 \u0434\u043b\u044f \u043e\u0445\u043b\u0430\u0436\u0434\u0435\u043d\u0438\u044f \u0430\u0433\u0440\u0435\u0433\u0430\u0442\u0430 \u00b0\u0421.", "title": "\u0443\u0442\u043e\u0447\u043d\u0435\u043d\u0438\u0435", "author": {"contactPoint": {"url": "http://www.pris.com.ua", "faxNumber": "+380487295093", "email": "amelina@pris.org.ua", "name": "\u0410\u043c\u0435\u043b\u0456\u043d\u0430 \u041a\u0430\u0442\u0435\u0440\u0438\u043d\u0