430\u0439\u043d\u043e\u0432\u0438\u0445 \u043f\u0440\u0430\u0432 \u0442\u0430 \u043f\u0440\u043e\u0444\u0435\u0441\u0456\u0439\u043d\u0443 \u043e\u0446\u0456\u043d\u043e\u0447\u043d\u0443 \u0434\u0456\u044f\u043b\u044c\u043d\u0456\u0441\u0442\u044c \u0432 \u0423\u043a\u0440\u0430\u0457\u043d\u0456\u00bb \u043d\u0430 \u0442\u0435\u043f\u043b\u043e\u0445\u043e\u0434\u0438:\n\u0442\\\u0445 \u00ab\u041c\u0430\u0448\u0443\u043a\u00bb - \u043c. \u0425\u0435\u0440\u0441\u043e\u043d;\n\u0442\\\u0445 \u00ab\u0414\u043e\u043c\u0431\u0430\u0439\u00bb - \u043c. \u0425\u0435\u0440\u0441\u043e\u043d;\n\u0442\\\u0445 \u00ab\u0411\u0435\u0448\u0442\u0430\u0443\u00bb - \u043c. \u0425\u0435\u0440\u0441\u043e\u043d;\n\u0442\\\u0445 \u00ab\u0422\u0430\u0442\u0440\u0438\u00bb - \u043c. \u0425\u0435\u0440\u0441\u043e\u043d;\n\u0442\\\u0445 \u00ab \u0410\u0440\u0430\u0440\u0430\u0442\u00bb - \u043c. \u0425\u0435\u0440\u0441\u043e\u043d;\n\u0442\\\u0445 \u00ab\u0420\u043e\u043c\u0430\u043d-\u041a\u043e\u0448\u00bb - \u043c. \u0425\u0435\u0440\u0441\u043e\u043d.\n\n\t\u041c\u0435\u0442\u0430 \u043f\u0440\u043e\u0432\u0435\u0434\u0435\u043d\u043d\u044f \u0437\u0430\u043a\u0443\u043f\u0456\u0432\u043b\u0456:  \u043f\u0440\u043e\u0432\u0435\u0434\u0435\u043d\u043d\u044f \u043d\u0435\u0437\u0430\u043b\u0435\u0436\u043d\u043e\u0457 \u043e\u0446\u0456\u043d\u043a\u0438  \u0432\u0438\u0449\u0435\u0432\u043a\u0430\u0437\u0430\u043d\u0438\u0445 \u0442\u0435\u043f\u043b\u043e\u0445\u043e\u0434\u0456\u0432, \u0437 \u0432\u0438\u0433\u043e\u0442\u043e\u0432\u043b\u0435\u043d\u043d\u044f\u043c \u0437\u0432\u0456\u0442\u0443 \u043f\u0440\u043e \u043e\u0446\u0456\u043d\u043a\u0443 \u0442\u0430 \u0440\u0435\u0446\u0435\u043d\u0437\u0456\u0454\u044e \u043d\u0430 \u0437\u0432\u0456\u0442 \u0432\u0456\u0434\u043f\u043e\u0432\u0456\u0434\u043d\u043e \u0434\u043e \u0417\u0423\u00bb\u041f\u0440\u043e \u043e\u0446\u0456\u043d\u043a\u0443 \u043c\u0430\u0439\u043d\u0430, \u043c\u0430\u0439\u043d\u043e\u0432\u0438\u0445 \u043f\u0440\u0430\u0432 \u0442\u0430 \u043f\u0440\u043e\u0444\u0435\u0441\u0456\u0439\u043d\u0443 \u043e\u0446\u0456\u043d\u043e\u0447\u043d\u0443 \u0434\u0456\u044f\u043b\u044c\u043d\u0456\u0441\u0442\u044c\u00bb \u0434\u043b\u044f \u0443\u043a\u043b\u0430\u0434\u0430\u043d\u043d\u044f \u0434\u043e\u0433\u043e\u0432\u043e\u0440\u0443 \u0431\u0435\u0440\u0431\u043e\u0443\u0442-\u0447\u0430\u0440\u0442\u0435\u0440\u0443.\n\u041c\u0456\u0441\u0446\u0435 \u043d\u0430\u0434\u0430\u043d\u043d\u044f \u043f\u043e\u0441\u043b\u0443\u0433: \u043c. \u0425\u0435\u0440\u0441\u043e\u043d\n\t5. \u0423\u0447\u0430\u0441\u043d\u0438\u043a \u0432\u0438\u0437\u043d\u0430\u0447\u0430\u0454 \u0446\u0456\u043d\u0443 \u0437 \u0443\u0440\u0430\u0445\u0443\u0432\u0430\u043d\u043d\u044f\u043c \u0443\u0441\u0456\u0445 \u0441\u0432\u043e\u0457\u0445 \u0432\u0438\u0442\u0440\u0430\u0442, \u043f\u043e\u0434\u0430\u0442\u043a\u0456\u0432 \u0456 \u0437\u0431\u043e\u0440\u0456\u0432, \u0449\u043e \u0441\u043f\u043b\u0430\u0447\u0443\u044e\u0442\u044c\u0441\u044f \u0430\u0431\u043e \u043c\u0430\u044e\u0442\u044c \u0431\u0443\u0442\u0438 \u0441\u043f\u043b\u0430\u0447\u0435\u043d\u0456.\n5.1. \u0423\u043c\u043e\u0432\u0438 \u0440\u043e\u0437\u0440\u0430\u0445\u0443\u043d\u043a\u0456\u0432: \u043f\u0456\u0441\u043b\u044f\u043f\u043b\u0430\u0442\u0430 (\u043f\u043e \u0444\u0430\u043a\u0442\u0443 \u043d\u0430\u0434\u0430\u043d\u043d\u044f \u043f\u043e\u0441\u043b\u0443\u0433).\n\t6. \u0412\u0438\u043c\u043e\u0433\u0438 \u0449\u043e\u0434\u043e \u044f\u043a\u043e\u0441\u0442\u0456 \u043f\u043e\u0441\u043b\u0443\u0433. \u042f\u043a\u0456\u0441\u0442\u044c \u043f\u043e\u0441\u043b\u0443\u0433, \u0449\u043e \u043d\u0430\u0434\u0430\u0454\u0442\u044c\u0441\u044f \u043c\u0430\u0454 \u0432\u0456\u0434\u043f\u043e\u0432\u0456\u0434\u0430\u0442\u0438 \u0447\u0438\u043d\u043d\u043e\u043c\u0443 \u0437\u0430\u043a\u043e\u043d\u043e\u0434\u0430\u0432\u0441\u0442\u0432\u0443 \u0423\u043a\u0440\u0430\u0457\u043d\u0438, \u0434\u043e \u0432\u043a\u0430\u0437\u0430\u043d\u043e\u0433\u043e \u0432\u0438\u0434\u0443 \u0434\u0456\u044f\u043b\u044c\u043d\u043e\u0441\u0442\u0456.\n\t7.  \u0421\u043f\u043e\u0441\u0456\u0431 \u043f\u0456\u0434\u0442\u0432\u0435\u0440\u0434\u0436\u0435\u043d\u043d\u044f \u0432\u0456\u0434\u043f\u043e\u0432\u0456\u0434\u043d\u043e\u0441\u0442\u0456 \u043f\u043e\u0441\u043b\u0443\u0433 \u0432\u0438\u043c\u043e\u0433\u0430\u043c.\n\t\u0423\u0447\u0430\u0441\u043d\u0438\u043a \u043f\u043e\u0432\u0438\u043d\u0435\u043d \u043d\u0430\u0434\u0430\u0442\u0438 \u0432 \u0435\u043b\u0435\u043a\u0442\u0440\u043e\u043d\u043d\u043e\u043c\u0443 (\u0441\u043a\u0430\u043d\u043e\u0432\u0430\u043d\u043e\u043c\u0443) \u0432\u0438\u0433\u043b\u044f\u0434\u0456 \u0432 \u0441\u043a\u043b\u0430\u0434\u0456 \u0441\u0432\u043e\u0454\u0457 \u043f\u0440\u043e\u043f\u043e\u0437\u0438\u0446\u0456\u0457, \u043f\u043e\u044f\u0441\u043d\u044e\u0432\u0430\u043b\u044c\u043d\u0443 \u0437\u0430\u043f\u0438\u0441\u043a\u0443 \u043d\u0430 \u0444\u0456\u0440\u043c\u043e\u0432\u043e\u043c\u0443 \u0431\u043b\u0430\u043d\u043a\u0443 \u0443\u0447\u0430\u0441\u043d\u0438\u043a\u0430 \u0437\u0430 \u043f\u0456\u0434\u043f\u0438\u0441\u043e\u043c \u0443\u043f\u043e\u0432\u043d\u043e\u0432\u0430\u0436\u0435\u043d\u043e\u0457 \u043e\u0441\u043e\u0431\u0438 \u0439 \u0437\u0430\u0432\u0456\u0440\u0435\u043d\u0443 \u043f\u0435\u0447\u0430\u0442\u043a\u043e\u044e.\n\t\u0422\u0430\u043a\u043e\u0436, \u043d\u0430 \u0440\u043e\u0437\u0441\u0443\u0434 \u0443\u0447\u0430\u0441\u043d\u0438\u043a\u0430, \u043c\u043e\u0436\u043b\u0438\u0432\u0435 \u043d\u0430\u0434\u0430\u043d\u043d\u044f \u0431\u0443\u0434\u044c-\u044f\u043a\u043e\u0457 \u0434\u043e\u0434\u0430\u0442\u043a\u043e\u0432\u043e\u0457 \u0456\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0456\u0457 \u0430\u0431\u043e \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u0456\u0432.\n\t8. \u041c\u0456\u0441\u0446\u0435  \u043f\u043e\u0434\u0430\u043d\u043d\u044f  \u0437\u0432\u0456\u0442\u0456\u0432 \u043f\u0440\u043e \u043d\u0435\u0437\u0430\u043b\u0435\u0436\u043d\u0443 \u043e\u0446\u0456\u043d\u043a\u0443 \u0442\u0435\u043f\u043b\u043e\u0445\u043e\u0434\u0456\u0432 \u0442\u0430 \u0440\u0435\u0446\u0435\u043d\u0437\u0456\u0439 \u0434\u043e \u0437\u0432\u0456\u0442\u0456\u0432: \u043c. \u041a\u0438\u0457\u0432, \u0437\u0430 \u0430\u0434\u0440\u0435\u0441\u043e\u044e \u043d\u0430\u0434\u0430\u043d\u043d\u044f \u043f\u043e\u0441\u043b\u0443\u0433\u0438. 10  \u0440\u043e\u0431\u043e\u0447\u0438\u0445 \u0434\u043d\u0456\u0432 \u0437 \u043c\u043e\u043c\u0435\u043d\u0442\u0443 \u043e\u0442\u0440\u0438\u043c\u0430\u043d\u043d\u044f \u0412\u0438\u043a\u043e\u043d\u0430\u0432\u0446\u0435\u043c \u0432\u0441\u0456\u0445 \u043d\u0435\u043e\u0431\u0445\u0456\u0434\u043d\u0438\u0445 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u0456\u0432 .\n10. \u0412\u0438\u043c\u043e\u0433\u0438 \u0434\u043e \u043a\u0432\u0430\u043b\u0456\u0444\u0456\u043a\u0430\u0446\u0456\u0457 \u0443\u0447\u0430\u0441\u043d\u0438\u043a\u0456\u0432 \u0442\u0430 \u0441\u043f\u043e\u0441\u0456\u0431 \u0457\u0445 \u043f\u0456\u0434\u0442\u0432\u0435\u0440\u0434\u0436\u0435\u043d\u043d\u044f.\n\u0423\u0447\u0430\u0441\u043d\u0438\u043a \u043f\u043e\u0432\u0438\u043d\u0435\u043d \u043d\u0430\u0434\u0430\u0442\u0438 \u0432 \u0435\u043b\u0435\u043a\u0442\u0440\u043e\u043d\u043d\u043e\u043c\u0443 (\u0441\u043a\u0430\u043d\u043e\u0432\u0430\u043d\u043e\u043c\u0443) \u0432\u0438\u0433\u043b\u044f\u0434\u0456 \u0432 \u0441\u043a\u043b\u0430\u0434\u0456 \u0441\u0432\u043e\u0454\u0457 \u043f\u0440\u043e\u043f\u043e\u0437\u0438\u0446\u0456\u0457 \u043d\u0430\u0441\u0442\u0443\u043f\u043d\u0456 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u0438:\n-  \u043a\u043e\u043f\u0456\u044e \u0432\u0438\u043f\u0438\u0441\u043a\u0438 \u0430\u0431\u043e \u0432\u0438\u0442\u044f\u0433\u0443 \u0437 \u0404\u0434\u0438\u043d\u043e\u0433\u043e \u0434\u0435\u0440\u0436\u0430\u0432\u043d\u043e\u0433\u043e \u0440\u0435\u0454\u0441\u0442\u0440\u0443 \u044e\u0440\u0438\u0434\u0438\u0447\u043d\u0438\u0445 \u043e\u0441\u0456\u0431 \u0442\u0430 \u0444\u0456\u0437\u0438\u0447\u043d\u0438\u0445 \u043e\u0441\u0456\u0431-\u043f\u0456\u0434\u043f\u0440\u0438\u0454\u043c\u0446\u0456\u0432, \u0441\u0442\u0430\u043d\u043e\u043c \u043d\u0435 \u0440\u0430\u043d\u0456\u0448\u0435 \u043d\u0456\u0436  \u0406V \u043a\u0432\u0430\u0440\u0442\u0430\u043b 2015 \u0440\u0456\u043a (\u0434\u043b\u044f \u044e\u0440\u0438\u0434\u0438\u0447\u043d\u0438\u0445 \u043e\u0441\u0456\u0431);\n-   \u043a\u043e\u043f\u0456\u044f \u0441\u0432\u0456\u0434\u043e\u0446\u0442\u0432\u0430 \u043f\u0440\u043e \u0440\u0435\u0454\u0441\u0442\u0440\u0430\u0446\u0456\u044e \u043f\u043b\u0430\u0442\u043d\u0438\u043a\u0430 \u043f\u043e\u0434\u0430\u0442\u043a\u0443 \u043d\u0430 \u0434\u043e\u0434\u0430\u043d\u0443 \u0432\u0430\u0440\u0442\u0456\u0441\u0442\u044c \u0442\u0430/\u0430\u0431\u043e \u0432\u0438\u0442\u044f\u0433 \u0437 \u0440\u0435\u0454\u0441\u0442\u0440\u0443 \u043f\u043b\u0430\u0442\u043d\u0438\u043a\u0456\u0432 \u041f\u0414\u0412 (\u044f\u043a\u0449\u043e \u0443\u0447\u0430\u0441\u043d\u0438\u043a \u0454 \u043f\u043b\u0430\u0442\u043d\u0438\u043a\u043e\u043c \u041f\u0414\u0412), \u0430\u0431\u043e \u0441\u0432\u0456\u0434\u043e\u0446\u0442\u0432\u043e \u043f\u0440\u043e \u0441\u043f\u043b\u0430\u0442\u0443 \u0454\u0434\u0438\u043d\u043e\u0433\u043e \u043f\u043e\u0434\u0430\u0442\u043a\u0443 (\u044f\u043a\u0449\u043e \u0443\u0447\u0430\u0441\u043d\u0438\u043a \u0454 \u043f\u043b\u0430\u0442\u043d\u0438\u043a\u043e\u043c \u0454\u0434\u0438\u043d\u043e\u0433\u043e \u043f\u043e\u0434\u0430\u0442\u043a\u0443);\n-  \u0434\u043e\u0432\u0456\u0434\u043a\u0443 \u043d\u0430 \u0444\u0456\u0440\u043c\u043e\u0432\u043e\u043c\u0443 \u0431\u043b\u0430\u043d\u043a\u0443 \u0443\u0447\u0430\u0441\u043d\u0438\u043a\u0430 \u0437\u0430 \u043f\u0456\u0434\u043f\u0438\u0441\u043e\u043c \u0443\u043f\u043e\u0432\u043d\u043e\u0432\u0430\u0436\u0435\u043d\u043e\u0457 \u043e\u0441\u043e\u0431\u0438 \u0439 \u0437\u0430\u0432\u0456\u0440\u0435\u043d\u0443 \u043f\u0435\u0447\u0430\u0442\u043a\u043e\u044e (\u043d\u0435 \u043e\u0431\u043e\u0432\u2019\u044f\u0437\u043a\u043e\u0432\u043e, \u0443 \u0432\u0438\u043f\u0430\u0434\u043a\u0443 \u0437\u0434\u0456\u0439\u0441\u043d\u0435\u043d\u043d\u044f \u0434\u0456\u044f\u043b\u044c\u043d\u043e\u0441\u0442\u0456 \u0431\u0435\u0437 \u043f\u0435\u0447\u0430\u0442\u043a\u0438), \u044f\u043a\u0430 \u043f\u043e\u0432\u0438\u043d\u043d\u0430 \u043c\u0456\u0441\u0442\u0438\u0442\u0438 \u043a\u043e\u043d\u0442\u0430\u043a\u0442\u043d\u0456 \u0434\u0430\u043d\u0456 \u0443\u0447\u0430\u0441\u043d\u0438\u043a\u0430 (\u0456\u0437 \u0437\u0430\u0437\u043d\u0430\u0447\u0435\u043d\u043d\u044f\u043c \u0440\u0435\u043a\u0432\u0456\u0437\u0438\u0442\u0456\u0432 \u0443\u0447\u0430\u0441\u043d\u0438\u043a\u0430: \u043d\u0430\u0439\u043c\u0435\u043d\u0443\u0432\u0430\u043d\u043d\u044f, \u043a\u043e\u0434\u0443 \u0404\u0414\u0420\u041f\u041e\u0423, \u0440\u043e\u0437\u0440\u0430\u0445\u0443\u043d\u043a\u043e\u0432\u0438\u0445 \u0440\u0435\u043a\u0432\u0456\u0437\u0438\u0442\u0456\u0432, \u043c\u0456\u0441\u0446\u0435\u0437\u043d\u0430\u0445\u043e\u0434\u0436\u0435\u043d\u043d\u044f, \u043f\u043e\u0448\u0442\u043e\u0432\u043e\u0457 \u0430\u0434\u0440\u0435\u0441\u0438, \u0442\u0435\u043b\u0435\u0444\u043e\u043d\u0443,  \u0435\u043b\u0435\u043a\u0442\u0440\u043e\u043d\u043d\u043e\u0457 \u0430\u0434\u0440\u0435\u0441\u0438, \u0432\u0456\u0434\u043e\u043c\u043e\u0441\u0442\u0435\u0439 \u043f\u0440\u043e \u043a\u043e\u043d\u0442\u0430\u043a\u0442\u043d\u0443 \u043e\u0441\u043e\u0431\u0443 (\u043f\u0440\u0456\u0437\u0432\u0438\u0449\u0435, \u0456\u043c'\u044f, \u043f\u043e-\u0431\u0430\u0442\u044c\u043a\u043e\u0432\u0456, \u043f\u043e\u0441\u0430\u0434\u0430, \u043a\u043e\u043d\u0442\u0430\u043a\u0442\u043d\u0438\u0439 \u0442\u0435\u043b\u0435\u0444\u043e\u043d).\n\t- \u043f\u043e\u0432\u043d\u0456\u0441\u0442\u044e \u0437\u0430\u043f\u043e\u0432\u043d\u0435\u043d\u0456 \u0442\u0430 \u043f\u0456\u0434\u043f\u0438\u0441\u0430\u043d\u0456  \u0443\u0447\u0430\u0441\u043d\u0438\u043a\u043e\u043c \u043a\u043e\u043d\u043a\u0443\u0440\u0441\u043d\u0456 \u0444\u043e\u0440\u043c\u0438   \u00ab\u041f\u0440\u043e\u043f\u043e\u0437\u0438\u0446\u0456\u044f\u00bb \u0437\u0433\u0456\u0434\u043d\u043e \u0434\u043e\u0434\u0430\u0442\u043a\u0443 1,   \u00ab\u0414\u043e\u0432\u0456\u0434\u043a\u0430 \u043f\u0440\u043e \u0432\u0456\u0434\u0441\u0443\u0442\u043d\u0456\u0441\u0442\u044c \u043f\u0456\u0434\u0441\u0442\u0430\u0432 \u0434\u043b\u044f \u0432\u0456\u0434\u043c\u043e\u0432\u0438 \u0432 \u0443\u0447\u0430\u0441\u0442\u0456 \u0443 \u043f\u0440\u043e\u0446\u0435\u0434\u0443\u0440\u0456 \u0437\u0430\u043a\u0443\u043f\u0456\u0432\u043b\u0456\u00bb,  \u0437\u0433\u0456\u0434\u043d\u043e \u0434\u043e\u0434\u0430\u0442\u043a\u0443 2 \u0446\u044c\u043e\u0433\u043e \u043e\u0433\u043e\u043b\u043e\u0448\u0435\u043d\u043d\u044f,  \n- \u043a\u043e\u043f\u0456\u044e \u0432\u0456\u0434\u043f\u043e\u0432\u0456\u0434\u043d\u0438\u0445 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u0456\u0432 \u0443 \u0432\u0438\u043f\u0430\u0434\u043a\u0443 \u0437\u0434\u0456\u0439\u0441\u043d\u0435\u043d\u043d\u044f \u0434\u0456\u044f\u043b\u044c\u043d\u043e\u0441\u0442\u0456, \u044f\u043a\u0430 \u0432\u0456\u0434\u043f\u043e\u0432\u0456\u0434\u043d\u043e \u0434\u043e \u0447\u0438\u043d\u043d\u043e\u0433\u043e \u0437\u0430\u043a\u043e\u043d\u043e\u0434\u0430\u0432\u0441\u0442\u0432\u0430 \u0437\u0434\u0456\u0439\u0441\u043d\u044e\u0454\u0442\u044c\u0441\u044f \u0437 \u043e\u0431\u043e\u0432\u2019\u044f\u0437\u043a\u043e\u0432\u0438\u043c \u043e\u0442\u0440\u0438\u043c\u0430\u043d\u043d\u044f\u043c \u0441\u0435\u0440\u0442\u0438\u0444\u0456\u043a\u0430\u0442\u0443 \u0442\u0430/\u0430\u0431\u043e \u043b\u0456\u0446\u0435\u043d\u0437\u0456\u0457.\n11. \u0414\u0430\u0442\u0430 \u0442\u0430 \u0447\u0430\u0441 \u0437\u0430\u043a\u0456\u043d\u0447\u0435\u043d\u043d\u044f  \u00ab22\u00bb \u0441\u0456\u0447\u043d\u044f 2016 \u0440\u043e\u043a\u0443 23 -00  \u0433\u043e\u0434.\n12. \u0414\u0430\u0442\u0430 \u0442\u0430 \u0447\u0430\u0441 \u043f\u043e\u0447\u0430\u0442\u043a\u0443 \u043f\u043e\u0434\u0430\u043d\u043d\u044f \u043f\u0440\u043e\u043f\u043e\u0437\u0438\u0446\u0456\u0439: \u00ab22\u00bb \u0441\u0456\u0447\u043d\u044f 2016 \u0440\u043e\u043a\u0443  \u043e\u0431 00-00  \u0433\u043e\u0434.\n13. \u0414\u0430\u0442\u0430 \u0442\u0430 \u0447\u0430\u0441 \u0437\u0430\u043a\u0456\u043d\u0447\u0435\u043d\u043d\u044f \u043f\u043e\u0434\u0430\u043d\u043d\u044f \u043f\u0440\u043e\u043f\u043e\u0437\u0438\u0446\u0456\u0439: \u00ab27\u00bb \u0441\u0456\u0447\u043d\u044f 2016 \u0440\u043e\u043a\u0443 \u043e\u0431 23-00  \u0433\u043e\u0434.\n14. \u0414\u0430\u0442\u0430 \u0442\u0430 \u0447\u0430\u0441 \u043f\u0440\u043e\u0432\u0435\u0434\u0435\u043d\u043d\u044f \u0435\u043b\u0435\u043a\u0442\u0440\u043e\u043d\u043d\u043e\u0433\u043e \u0440\u0435\u0432\u0435\u0440\u0441\u0438\u0432\u043d\u043e\u0433\u043e \u0430\u0443\u043a\u0446\u0456\u043e\u043d\u0443: \u0434\u043e \u0437\u0430\u043a\u0456\u043d\u0447\u0435\u043d\u043d\u044f \u0447\u0430\u0441\u0443 \u0442\u0430 \u0434\u0430\u0442\u0438 \u043f\u043e\u0434\u0430\u043d\u043d\u044f \u043f\u0440\u043e\u043f\u043e\u0437\u0438\u0446\u0456\u0439.\n\u041a\u0440\u043e\u043a \u0430\u0443\u043a\u0446\u0456\u043e\u043d\u0443 1% \u043e\u0447\u0456\u043a\u0443\u0432\u0430\u043d\u043e\u0457 \u0432\u0430\u0440\u0442\u043e\u0441\u0442\u0456 \u0442\u043e\u0432\u0430\u0440\u0443 \u2013 120 \u0433\u0440\u043d.  \n15. \u0406\u043d\u0448\u0430 \u0456\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0456\u044f:\n \u0423\u0447\u0430\u0441\u043d\u0438\u043a \u043f\u0440\u043e\u0442\u044f\u0433\u043e\u043c 2-\u0445 \u0440\u043e\u0431\u043e\u0447\u0438\u0445 \u0434\u043d\u0456\u0432 \u0434\u043b\u044f \u0432\u0438\u0437\u043d\u0430\u0447\u0435\u043d\u043d\u044f \u041f\u0435\u0440\u0435\u043c\u043e\u0436\u0446\u044f \u043f\u043e\u0432\u0438\u043d\u0435\u043d \u043d\u0430\u0434\u0430\u0442\u0438  \u0443 \u043f\u0430\u043f\u0435\u0440\u043e\u0432\u043e\u043c\u0443 \u0432\u0438\u0433\u043b\u044f\u0434\u0456 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u0438  \u0437\u0433\u0456\u0434\u043d\u043e \u043f.10 \u0434\u0430\u043d\u043e\u0433\u043e \u043e\u0433\u043e\u043b\u043e\u0448\u0435\u043d\u043d\u044f. \u0412\u0441\u0456 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u0438 \u044f\u043a\u0456 \u043f\u043e\u0434\u0430\u0454  \u0443\u0447\u0430\u0441\u043d\u0438\u043a(\u043f\u0435\u0440\u0435\u043c\u043e\u0436\u0435\u0446\u044c)  \u043f\u043e\u0432\u0438\u043d\u043d\u0456 \u043d\u0430 \u0432\u0441\u0456\u0445 \u0441\u0442\u043e\u0440\u0456\u043d\u043a\u0430\u0445 \u043c\u0430\u0442\u0438 \u0432\u0456\u0434\u0431\u0438\u0442\u043e\u043a \u043f\u0435\u0447\u0430\u0442\u043a\u0438 \u0443\u0447\u0430\u0441\u043d\u0438\u043a\u0430 \u0442\u0430 \u043f\u0456\u0434\u043f\u0438\u0441 \u0443\u043f\u043e\u0432\u043d\u043e\u0432\u0430\u0436\u0435\u043d\u043e\u0457 \u043e\u0441\u043e\u0431\u0438 (\u0437\u0430\u0441\u0432\u0456\u0434\u0447\u0435\u043d\u0456 \u043d\u0430\u043b\u0435\u0436\u043d\u0438\u043c \u0447\u0438\u043d\u043e\u043c: -\u0437\u0456 \u0441\u043b\u0456\u0432 \"\u0417\u0433\u0456\u0434\u043d\u043e \u0437 \u043e\u0440\u0438\u0433\u0456\u043d\u0430\u043b\u043e\u043c\", \u043d\u0430\u0437\u0432\u0438 \u043f\u043e\u0441\u0430\u0434\u0438, \u043e\u0441\u043e\u0431\u0438\u0441\u0442\u043e\u0433\u043e \u043f\u0456\u0434\u043f\u0438\u0441\u0443 \u043e\u0441\u043e\u0431\u0438, \u044f\u043a\u0430 \u0437\u0430\u0441\u0432\u0456\u0434\u0447\u0443\u0454 \u043a\u043e\u043f\u0456\u044e, \u0457\u0457 \u0456\u043d\u0456\u0446\u0456\u0430\u043b\u0456\u0432 \u0442\u0430 \u043f\u0440\u0456\u0437\u0432\u0438\u0449\u0430, \u0434\u0430\u0442\u0438 \u0437\u0430\u0441\u0432\u0456\u0434\u0447\u0435\u043d\u