435\u043b\u0456 \u0433\u0456\u043f\u0441\u043e\u043a\u0430\u0440\u0442\u043e\u043d\u043d\u0438\u043c\u0438 \u0430\u0431\u043e \u0433\u0456\u043f\u0441\u043e\u0432\u043e\u043b\u043e\u043a\u043d\u0438\u0441\u0442\u0438\u043c\u0438 \u043b\u0438\u0441\u0442\u0430\u043c\u0438 (\u043e\u0447\u0438\u0449\u0435\u043d\u043d\u044f, \u0448\u043f\u0430\u043a\u043b\u044e\u0432\u0430\u043d\u043d\u044f, \u0491\u0440\u0443\u043d\u0442\u0443\u0432\u0430\u043d\u043d\u044f, \u0444\u0430\u0440\u0431\u0443\u0432\u0430\u043d\u043d\u044f, \u043f\u043e\u043b\u0456\u043f\u0448\u0435\u043d\u0435 \u0444\u0430\u0440\u0431\u0443\u0432\u0430\u043d\u043d\u044f \u043f\u043e\u043b\u0456\u0432\u0456\u043d\u0456\u043b\u0430\u0446\u0435\u0442\u0430\u0442\u043d\u0438\u043c\u0438 \u0432\u043e\u0434\u043e\u0435\u043c\u0443\u043b\u044c\u0441\u0456\u0439\u043d\u0438\u043c\u0438 \u0441\u0443\u043c\u0456\u0448\u0430\u043c\u0438).\n2.\t\u0420\u0435\u043c\u043e\u043d\u0442 \u0441\u0442\u0456\u043d (\u043e\u0447\u0438\u0449\u0435\u043d\u043d\u044f, \u0440\u0435\u043c\u043e\u043d\u0442 \u0448\u0442\u0443\u043a\u0430\u0442\u0443\u0440\u043a\u0438 \u043f\u043e \u043a\u0430\u043c\u0435\u043d\u044e \u0442\u0430 \u0431\u0435\u0442\u043e\u043d\u0443, \u0448\u043f\u0430\u043a\u043b\u044e\u0432\u0430\u043d\u043d\u044f, \u0491\u0440\u0443\u043d\u0442\u0443\u0432\u0430\u043d\u043d\u044f, \u0443\u043a\u043b\u0430\u0434\u0430\u043d\u043d\u044f \u0441\u0456\u0442\u043a\u0438 \u0441\u043a\u043b\u044f\u043d\u043e\u0457 \u0431\u0443\u0434\u0456\u0432\u0435\u043b\u044c\u043d\u043e\u0457, \u043f\u043e\u043b\u0456\u043f\u0448\u0435\u043d\u0435 \u0444\u0430\u0440\u0431\u0443\u0432\u0430\u043d\u043d\u044f \u043a\u043e\u043b\u0435\u0440\u043e\u043c \u043e\u043b\u0456\u0439\u043d\u0438\u043c, \u0440\u043e\u0437\u0434\u0440\u0456\u0431\u043b\u0435\u043d\u0438\u043c \u043f\u043e \u0437\u0431\u0456\u0440\u043d\u0438\u0445 \u043a\u043e\u043d\u0441\u0442\u0440\u0443\u043a\u0446\u0456\u044f\u0445, \u043f\u0456\u0434\u0433\u043e\u0442\u043e\u0432\u043b\u0435\u043d\u0438\u0445 \u043f\u0456\u0434 \u0444\u0430\u0440\u0431\u0443\u0432\u0430\u043d\u043d\u044f, \u043f\u043e\u043b\u0456\u043f\u0448\u0435\u043d\u0435 \u0444\u0430\u0440\u0431\u0443\u0432\u0430\u043d\u043d\u044f \u043f\u043e\u043b\u0456\u0432\u0456\u043d\u0456\u043b\u0430\u0446\u0435\u0442\u0430\u0442\u043d\u0438\u043c\u0438 \u0432\u043e\u0434\u043e\u0435\u043c\u0443\u043b\u044c\u0441\u0456\u0439\u043d\u0438\u043c\u0438 \u0441\u0443\u043c\u0456\u0448\u0430\u043c\u0438).\n3.\t\u0412\u0438\u043a\u043e\u043d\u0430\u043d\u043d\u044f \u0432\u0456\u043a\u043e\u043d\u043d\u0438\u0445 \u0443\u043a\u043e\u0441\u0456\u0432 (\u0443\u043b\u0430\u0448\u0442\u0443\u0432\u0430\u043d\u043d\u044f \u043e\u0431\u0448\u0438\u0432\u043a\u0438 \u0443\u043a\u043e\u0441\u0456\u0432 \u0433\u0456\u043f\u0441\u043e\u043a\u0430\u0440\u0442\u043e\u043d\u043d\u0438\u043c\u0438 \u043f\u043b\u0438\u0442\u0430\u043c\u0438 (\u0444\u0430\u043b\u044c\u0448\u0441\u0442\u0456\u043d\u0438) \u043f\u043e \u043c\u0435\u0442\u0430\u043b\u0435\u0432\u043e\u043c\u0443 \u043a\u0430\u0440\u043a\u0430\u0441\u0443, \u0442\u0435\u043f\u043b\u043e\u0456\u0437\u043e\u043b\u044f\u0446\u0456\u044f, \u0448\u043f\u0430\u043a\u043b\u044e\u0432\u0430\u043d\u043d\u044f, \u0491\u0440\u0443\u043d\u0442\u0443\u0432\u0430\u043d\u043d\u044f, \u043f\u043e\u043b\u0456\u043f\u0448\u0435\u043d\u0435 \u0444\u0430\u0440\u0431\u0443\u0432\u0430\u043d\u043d\u044f \u043f\u043e\u043b\u0456\u0432\u0456\u043d\u0456\u043b\u0430\u0446\u0435\u0442\u0430\u0442\u043d\u0438\u043c\u0438 \u0432\u043e\u0434\u043e\u0435\u043c\u0443\u043b\u044c\u0441\u0456\u0439\u043d\u0438\u043c\u0438 \u0441\u0443\u043c\u0456\u0448\u0430\u043c\u0438).\n4.\t\u0412\u0438\u043a\u043e\u043d\u0430\u043d\u043d\u044f \u0434\u0432\u0435\u0440\u043d\u0438\u0445 \u0443\u043a\u043e\u0441\u0456\u0432 (\u0440\u0435\u043c\u043e\u043d\u0442 \u0448\u0442\u0443\u043a\u0430\u0442\u0443\u0440\u043a\u0438 \u043f\u0440\u044f\u043c\u043e\u043b\u0456\u043d\u0456\u0439\u043d\u0438\u0445 \u0443\u043a\u043e\u0441\u0456\u0432 \u0432\u0441\u0435\u0440\u0435\u0434\u0438\u043d\u0456 \u0431\u0443\u0434\u0456\u0432\u043b\u0456 \u043f\u043e \u043a\u0430\u043c\u0435\u043d\u044e \u0442\u0430 \u0431\u0435\u0442\u043e\u043d\u0443 \u0441\u0443\u043c\u0456\u0448\u0430\u043c\u0438 \u0441\u0443\u0445\u0438\u043c\u0438 \u0448\u0442\u0443\u043a\u0430\u0442\u0443\u0440\u043d\u0438\u043c\u0438, \u0443\u043b\u0430\u0448\u0442\u0443\u0432\u0430\u043d\u043d\u044f \u043e\u0431\u0448\u0438\u0432\u043a\u0438 \u0443\u043a\u043e\u0441\u0456\u0432 \u0433\u0456\u043f\u0441\u043e\u043a\u0430\u0440\u0442\u043e\u043d\u043d\u0438\u043c\u0438 \u043f\u043b\u0438\u0442\u0430\u043c\u0438 (\u0444\u0430\u043b\u044c\u0448\u0441\u0442\u0456\u043d\u0438) \u043f\u043e \u043c\u0435\u0442\u0430\u043b\u0435\u0432\u043e\u043c\u0443 \u043a\u0430\u0440\u043a\u0430\u0441\u0443, \u0442\u0435\u043f\u043b\u043e\u0456\u0437\u043e\u043b\u044f\u0446\u0456\u044f, \u0448\u043f\u0430\u043a\u043b\u044e\u0432\u0430\u043d\u043d\u044f, \u0491\u0440\u0443\u043d\u0442\u0443\u0432\u0430\u043d\u043d\u044f, \u043f\u043e\u043b\u0456\u043f\u0448\u0435\u043d\u0435 \u0444\u0430\u0440\u0431\u0443\u0432\u0430\u043d\u043d\u044f \u043a\u043e\u043b\u0435\u0440\u043e\u043c \u043e\u043b\u0456\u0439\u043d\u0438\u043c \u0440\u043e\u0437\u0431\u0456\u043b\u0435\u043d\u0438\u043c \u0443\u043a\u043e\u0441\u0456\u0432 \u043f\u043e \u0437\u0431\u0456\u0440\u043d\u0438\u0445 \u043a\u043e\u043d\u0441\u0442\u0440\u0443\u043a\u0446\u0456\u044f\u0445, \u043f\u0456\u0434\u0433\u043e\u0442\u043e\u0432\u043b\u0435\u043d\u0438\u0445 \u043f\u0456\u0434 \u0444\u0430\u0440\u0431\u0443\u0432\u0430\u043d\u043d\u044f, \u043f\u043e\u043b\u0456\u043f\u0448\u0435\u043d\u0435 \u0444\u0430\u0440\u0431\u0443\u0432\u0430\u043d\u043d\u044f \u043f\u043e\u043b\u0456\u0432\u0456\u043d\u0456\u043b\u0430\u0446\u0435\u0442\u0430\u0442\u043d\u0438\u043c\u0438 \u0432\u043e\u0434\u043e\u0435\u043c\u0443\u043b\u044c\u0441\u0456\u0439\u043d\u0438\u043c\u0438 \u0441\u0443\u043c\u0456\u0448\u0430\u043c\u0438 \u0443\u043a\u043e\u0441\u0456\u0432 \u043f\u043e \u0437\u0431\u0456\u0440\u043d\u0438\u0445 \u043a\u043e\u043d\u0441\u0442\u0440\u0443\u043a\u0446\u0456\u044f\u0445).\n5.\t\u0420\u0435\u043a\u043e\u043d\u0441\u0442\u0440\u0443\u043a\u0446\u0456\u044f \u0442\u0430 \u0443\u0442\u0435\u043f\u043b\u0435\u043d\u043d\u044f \u0434\u0432\u0435\u0440\u0435\u0439 (\u043f\u043e\u043b\u0456\u043f\u0448\u0435\u043d\u0435 \u043e\u043b\u0456\u0439\u043d\u0435 \u0444\u0430\u0440\u0431\u0443\u0432\u0430\u043d\u043d\u044f \u0440\u0430\u043d\u0456\u0448\u0435 \u043f\u043e\u0444\u0430\u0440\u0431\u043e\u0432\u0430\u043d\u0438\u0445 \u0434\u0432\u0435\u0440\u0435\u0439 \u0443\u0441\u0435\u0440\u0435\u0434\u0438\u043d\u0456 \u0431\u0443\u0434\u0456\u0432\u043b\u0456 \u0431\u0456\u043b\u0438\u043b\u043e\u043c \u0437 \u0440\u043e\u0437\u0447\u0438\u0449\u0435\u043d\u043d\u044f\u043c \u0441\u0442\u0430\u0440\u043e\u0457 \u0444\u0430\u0440\u0431\u0438 \u0434\u043e 35%, \u0442\u0435\u043f\u043b\u043e\u0456\u0437\u043e\u043b\u044f\u0446\u0456\u044f \u0434\u0432\u0435\u0440\u0435\u0439  \u0432\u0438\u0440\u043e\u0431\u0430\u043c\u0438 \u0437 \u043f\u0456\u043d\u043e\u043f\u043b\u0430\u0441\u0442\u0443 \u043d\u0430 \u043a\u043b\u0435\u044e, \u043e\u0431\u0448\u0438\u0432\u0430\u043d\u043d\u044f \u0434\u0432\u0435\u0440\u0435\u0439 \u0432\u0430\u0433\u043e\u043d\u043a\u043e\u044e, \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u043d\u044f \u0456 \u043a\u0440\u0456\u043f\u043b\u0435\u043d\u043d\u044f \u043d\u0430\u043b\u0438\u0447\u043d\u0438\u043a\u0456\u0432 (\u043b\u0438\u0448\u0442\u0432\u0438)).\n6.\t\u0420\u0435\u043c\u043e\u043d\u0442 \u043f\u0456\u0434\u043b\u043e\u0433\u0438 (\u0440\u043e\u0437\u0431\u0438\u0440\u0430\u043d\u043d\u044f \u0434\u0435\u0440\u0435\u0432'\u044f\u043d\u0438\u0445 \u043f\u043b\u0456\u043d\u0442\u0443\u0441\u0456\u0432, \u0440\u043e\u0437\u0431\u0438\u0440\u0430\u043d\u043d\u044f \u043f\u043e\u043a\u0440\u0438\u0442\u0442\u0456\u0432 \u043f\u0456\u0434\u043b\u043e\u0433 \u0437 \u043b\u0456\u043d\u043e\u043b\u0435\u0443\u043c\u0443 \u0442\u0430 \u0440\u0435\u043b\u0456\u043d\u0443, \u0440\u043e\u0437\u0431\u0438\u0440\u0430\u043d\u043d\u044f \u0446\u0435\u043c\u0435\u043d\u0442\u043d\u043e\u0457 \u0441\u0442\u044f\u0436\u043a\u0438, \u0443\u043b\u0430\u0448\u0442\u0443\u0432\u0430\u043d\u043d\u044f \u0446\u0435\u043c\u0435\u043d\u0442\u043d\u043e\u0457 \u0441\u0442\u044f\u0436\u043a\u0438 \u0442\u043e\u0432\u0449\u0438\u043d\u043e\u044e 20 \u043c\u043c \u043f\u043e \u0431\u0435\u0442\u043e\u043d\u043d\u0456\u0439 \u043e\u0441\u043d\u043e\u0432\u0456, \u0443\u043b\u0430\u0448\u0442\u0443\u0432\u0430\u043d\u043d\u044f \u043f\u043e\u043a\u0440\u0438\u0442\u0442\u0456\u0432 \u043f\u043e\u043b\u0456\u043c\u0435\u0440\u0446\u0435\u043c\u0435\u043d\u0442\u043d\u0438\u0445 \u043e\u0434\u043d\u043e\u0448\u0430\u0440\u043e\u0432\u0438\u0445 \u043d\u0430\u043b\u0438\u0432\u043d\u0438\u0445 \u0442\u043e\u0432\u0449\u0438\u043d\u043e\u044e 4 \u043c\u043c, \u0491\u0440\u0443\u043d\u0442\u0443\u0432\u0430\u043d\u043d\u044f, \u0443\u043b\u0430\u0448\u0442\u0443\u0432\u0430\u043d\u043d\u044f \u043f\u043e\u043a\u0440\u0438\u0442\u0442\u044f \u0437 \u043b\u0456\u043d\u043e\u043b\u0435\u0443\u043c\u0443, \u0443\u043b\u0430\u0448\u0442\u0443\u0432\u0430\u043d\u043d\u044f \u043f\u043b\u0456\u043d\u0442\u0443\u0441\u0456\u0432 \u0434\u0435\u0440\u0435\u0432\u2019\u044f\u043d\u0438\u0445).\n7.\t\u0420\u0456\u0437\u043d\u0456 \u0440\u043e\u0431\u043e\u0442\u0438 (\u0444\u0430\u0440\u0431\u0443\u0432\u0430\u043d\u043d\u044f \u043e\u043b\u0456\u0439\u043d\u0438\u043c\u0438 \u0441\u0443\u043c\u0456\u0448\u0430\u043c\u0438 \u0437\u0430 2 \u0440\u0430\u0437\u0438 \u0440\u0430\u043d\u0456\u0448\u0435 \u043f\u043e\u0444\u0430\u0440\u0431\u043e\u0432\u0430\u043d\u0438\u0445 \u043c\u0435\u0442\u0430\u043b\u0435\u0432\u0438\u0445 \u043f\u043e\u0432\u0435\u0440\u0445\u043e\u043d\u044c \u0433\u0440\u0430\u0442 (\u0432\u0456\u043a\u043e\u043d\u043d\u0456 \u0440\u0435\u0448\u0456\u0442\u043a\u0438), \u0444\u0430\u0440\u0431\u0443\u0432\u0430\u043d\u043d\u044f \u043e\u043b\u0456\u0439\u043d\u0438\u043c\u0438 \u0441\u0443\u043c\u0456\u0448\u0430\u043c\u0438 \u0437\u0430 2 \u0440\u0430\u0437\u0438 \u0440\u0430\u043d\u0456\u0448\u0435 \u043f\u043e\u0444\u0430\u0440\u0431\u043e\u0432\u0430\u043d\u0438\u0445 \u0440\u0430\u0434\u0456\u0430\u0442\u043e\u0440\u0456\u0432, \u0444\u0430\u0440\u0431\u0443\u0432\u0430\u043d\u043d\u044f \u043e\u043b\u0456\u0439\u043d\u0438\u043c\u0438 \u0441\u0443\u043c\u0456\u0448\u0430\u043c\u0438 \u0437\u0430 2 \u0440\u0430\u0437\u0438 \u0440\u0430\u043d\u0456\u0448\u0435 \u043f\u043e\u0444\u0430\u0440\u0431\u043e\u0432\u0430\u043d\u0438\u0445 \u0441\u0442\u0430\u043b\u0435\u0432\u0438\u0445 \u0442\u0440\u0443\u0431,  \u043f\u0435\u0440\u0435\u0432\u0435\u0437\u0435\u043d\u043d\u044f \u0441\u043c\u0456\u0442\u0442\u044f).\n8.\t\u041c\u043e\u043d\u0442\u0430\u0436\u043d\u0456 \u0440\u043e\u0431\u043e\u0442\u0438 (\u043c\u043e\u043d\u0442\u0430\u0436 \u0441\u0432\u0456\u0442\u0438\u043b\u044c\u043d\u0438\u043a\u0456\u0432 \u0434\u043b\u044f \u043b\u044e\u043c\u0456\u043d\u0435\u0441\u0446\u0435\u043d\u0442\u043d\u0438\u0445 \u043b\u0430\u043c\u043f \u0430\u0431\u043e \u0441\u0432\u0456\u0442\u043b\u043e\u0434\u0456\u043e\u0434\u043d\u0438\u0445, \u044f\u043a\u0456 \u0432\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u044e\u044e\u0442\u044c\u0441\u044f \u0432 \u043f\u0456\u0434\u0432\u0456\u0441\u043d\u0438\u0445 \u0441\u0442\u0435\u043b\u044f\u0445, \u0443\u0441\u0442\u0430\u043d\u043e\u0432\u043b\u0435\u043d\u043d\u044f \u0432\u0438\u043c\u0438\u043a\u0430\u0447\u0456\u0432).\n\u041d\u0430\u0434\u0430\u0454\u0442\u044c\u0441\u044f \u043c\u043e\u0436\u043b\u0438\u0432\u0456\u0441\u0442\u044c \u043e\u0433\u043b\u044f\u0434\u0443 \u043f\u0440\u0438\u043c\u0456\u0449\u0435\u043d\u043d\u044f \u0437\u0430 \u043f\u043e\u043f\u0435\u0440\u0435