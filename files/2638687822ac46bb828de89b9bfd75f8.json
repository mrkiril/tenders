u044c\u0441\u044f \u0437 \u043c\u0435\u0442\u0430\u043b\u0438\u0447\u043d\u043e\u0433\u043e \u043f\u0440\u043e\u0444\u0438\u043b\u044e \u0442\u0430 \u043b\u0430\u043c\u0435\u043b\u0456\u0439. \u041a\u043e\u043b\u0456\u0440 \u043b\u0430\u043c\u0435\u043b\u0456\u0439 \u2013 \u043f\u043e\u043f\u0456\u043b \u0442\u0440\u043e\u044f\u043d\u0434\u0438. \u0428\u0438\u0440\u0438\u043d\u0430 \u043f\u0440\u043e\u0444\u0438\u043b\u044f: \u0441\u0442\u0430\u043d\u0434\u0430\u0440\u0442, \u0432\u0438\u0441\u043e\u0442\u0430: \u0441\u0442\u0430\u043d\u0434\u0430\u0440\u0442, \u0448\u0438\u0440\u0438\u043d\u0430 \u043b\u0430\u043c\u0435\u043b\u0456\u0439 \u2013 90 \u043c\u043c , \u0432\u0438\u0441\u043e\u0442\u0430 \u043b\u0430\u043c\u0435\u043b\u0456\u0439  - 260\u043c\u043c,  1 \u0432\u0456\u043a\u043d\u043e \u2013 1,7 \u043c. \t\t\n3. \u0413\u043e\u0434\u0438\u043d\u043d\u0438\u043a \u043d\u0430\u0441\u0442\u0456\u043d\u0438\u0439 \u043a\u0432\u0430\u0440\u0446\u0435\u0432\u0438\u0439 \u0437 \u043f\u043b\u0430\u0432\u043d\u0438\u043c \u0445\u043e\u0434\u043e\u043c, \u0434\u0456\u0432\u043c\u0435\u0442\u0440\u043e\u043c 40 \u0441\u043c, \u0437\u0430\u043a\u0440\u0438\u0442\u0456 \u0441\u043a\u043b\u043e\u043c, \u043a\u043e\u043b\u0456\u0440 \u0441\u0440\u0456\u0431\u043d\u0438\u0439\t\t\n4. \u0419\u043e\u0440\u0448 \u043f\u0440\u0438\u0437\u043d\u0430\u0447\u0435\u043d\u0438\u0439 \u0434\u043b\u044f \u0447\u0438\u0449\u0435\u043d\u043d\u044f \u0441\u0430\u043d\u0442\u0435\u0445\u043d\u0456\u043a\u0438 \u0432 \u0442\u0443\u0430\u043b\u0435\u0442\u043d\u0456\u0439 \u043a\u0456\u043c\u043d\u0430\u0442\u0456. \u041a\u043e\u043c\u043f\u043b\u0435\u043a\u0442\u043d\u0456\u0441\u0442\u044c: \u0439\u043e\u0440\u0448\u0438\u043a \u0442\u0430 \u043f\u0456\u0434\u0441\u0442\u0430\u0432\u043a\u0430 \u0434\u043b\u044f \u043d\u044c\u043e\u0433\u043e, \u0440\u043e\u0437\u0442\u0430\u0448\u0443\u0432\u0430\u043d\u043d\u044f \u043f\u0456\u0434\u043b\u043e\u0433\u043e\u0432\u0435.\t\t\n5. \u0428\u0432\u0430\u0431\u0440\u0430 (\u0439\u043e\u0440\u0448 \u043f\u0440\u0438\u0437\u043d\u0430\u0447\u0435\u043d\u0438\u0439 \u0434\u043b\u044f \u0447\u0438\u0449\u0435\u043d\u043d\u044f \u0441\u0430\u043d\u0442\u0435\u0445\u043d\u0456\u043a\u0438 \u0432 \u0442\u0443\u0430\u043b\u0435\u0442\u043d\u0456\u0439 \u043a\u0456\u043c\u043d\u0430\u0442\u0456. \u041a\u043e\u043c\u043f\u043b\u0435\u043a\u0442\u043d\u0456\u0441\u0442\u044c: \u0439\u043e\u0440\u0448\u0438\u043a \u0442\u0430 \u043f\u0456\u0434\u0441\u0442\u0430\u0432\u043a\u0430 \u0434\u043b\u044f \u043d\u044c\u043e\u0433\u043e, \u0440\u043e\u0437\u0442\u0430\u0448\u0443\u0432\u0430\u043d\u043d\u044f \u043f\u0456\u0434\u043b\u043e\u0433\u043e\u0432\u0435).\t\t\n6. \u0428\u0432\u0430\u0431\u0440\u0430 (\u0449\u0456\u0442\u043a\u0430 \u0434\u043b\u044f \u043c\u0438\u0439\u043a\u0438 \u0434\u0443\u0448\u0435\u0432\u0438\u0445, \u0449\u0456\u0442\u043a\u0430 \u0437 \u0434\u0435\u0440\u0436\u0430\u043a\u043e\u043c \u044f\u043a\u0438\u0439 \u043a\u0440\u0456\u043f\u0438\u0442\u044c\u0441\u044f \u043f\u0456\u0434 \u0443\u0445\u0438\u043b\u043e\u043c, \u0434\u043b\u044f \u043c\u0438\u0439\u043a\u0438 \u043f\u0456\u0434\u043b\u043e\u0433\u0438 \u0442\u0430 \u0441\u0442\u0456\u043d \u0437 \u0434\u0440\u0456\u0431\u043d\u0438\u043c\u0438 \u0448\u0432\u0430\u043c\u0438. \u0416\u043e\u0440\u0441\u0442\u043a\u0438\u0439 \u043d\u0435\u0439\u043b\u043e\u043d\u043e\u0432\u0438\u0439 \u0432\u043e\u0440\u0441 \u043f\u043e\u0432\u0438\u043d\u0435\u043d \u043d\u0430\u0434\u0456\u0439\u043d\u043e \u043a\u0440\u0456\u043f\u0438\u0442\u044c\u0441\u044f \u0434\u043e \u043a\u043e\u043b\u043e\u0434\u043a\u0438, \u0449\u043e \u0437\u0430\u043f\u043e\u0431\u0456\u0433\u0430\u0454 \u043f\u0435\u0440\u0435\u0434\u0447\u0430\u0441\u043d\u043e\u043c\u0443 \u0432\u0438\u0445\u043e\u0434\u0443 \u0437 \u0454\u043a\u0441\u043f\u043b\u0443\u0430\u0442\u0430\u0446\u0456\u0457. \u0429\u0456\u0442\u043a\u0438\u043f\u043e\u0432\u0438\u043d\u0456 \u0431\u04