u0431\u043e\u043d\u0430\u0442 \u043d\u0430\u0442\u0440\u0456\u044e-\u043d\u0435 \u043c\u0435\u043d\u0448\u0435 30%,\u043a\u0430\u0440\u0431\u043e\u043d\u0430\u0442 \u043d\u0430\u0442\u0440\u0456\u044e-\u043d\u0435 \u043c\u0435\u043d\u0448\u0435 5%.\n3. \u0422\u0435\u043c\u043f\u0435\u0440\u0430\u0442\u0443\u0440\u043d\u043e\u0441\u0442\u0456\u0439\u043a\u0438\u0439 \u043f\u0440\u0435\u043f\u0430\u0440\u0430\u0442-\u0442\u0435\u043c\u043f\u0435\u0440\u0430\u0442\u0443\u0440\u0430 \u0440\u043e\u0437\u043a\u043b\u0430\u0434\u0430\u043d\u043d\u044f \u043d\u0435 \u043c\u0435\u043d\u0448\u0435 225\u00b0\u0421.\n4. \u0427\u0430\u0441 \u0440\u043e\u0437\u0447\u0438\u043d\u0435\u043d\u043d\u044f \u0443 \u0432\u043e\u0434\u0456 \u043a\u0456\u043c\u043d\u0430\u0442\u043d\u043e\u0457 \u0442\u0435\u043c\u043f\u0435\u0440\u0430\u0442\u0443\u0440\u0438-\u043d\u0435 \u0431\u0456\u043b\u044c\u0448\u0435 3 \u0445\u0432\u0438\u043b\u0438\u043d.\n5. \u0420\u043e\u0431\u043e\u0447\u0456 \u0440\u043e\u0437\u0447\u0438\u043d\u0438 \u043f\u043e\u0432\u0438\u043d\u043d\u0456 \u043c\u0430\u0442\u0438 \u043c\u0438\u044e\u0447\u0456 \u0432\u043b\u0430\u0441\u0442\u0438\u0432\u043e\u0441\u0442\u0456.\n6. \u0420\u043e\u0431\u043e\u0447\u0456 \u0440\u043e\u0437\u0447\u0438\u043d\u0438 \u043d\u0435 \u043f\u043e\u0432\u0438\u043d\u043d\u0456  \u043c\u0430\u0442\u0438 \u0441\u0435\u043d\u0441\u0438\u0431\u0456\u043b\u0456\u0437\u0443\u044e\u0447\u043e\u0457,\u043a\u0430\u043d\u0446\u0435\u0440\u043e\u0433\u0435\u043d\u043d\u043e\u0457 \u0442\u0430 \u043c\u0443\u0442\u0430\u0433\u0435\u043d\u043d\u043e\u0457 \u0434\u0456\u0457.\n7.\u0428\u0438\u0440\u043e\u043a\u0438\u0439 \u0441\u043f\u0435\u043a\u0442\u0440 \u0430\u043d\u0442\u0438\u043c\u0456\u043a\u0440\u043e\u0431\u043d\u043e\u0457 \u0434\u0456\u0457-\u0430\u043a\u0442\u0438\u0432\u043d\u0438\u0439 \u0449\u043e\u0434\u043e \u0431\u0430\u043a\u0442\u0435\u0440\u0456\u0439 (\u0432\u043a\u043b\u044e\u0447\u0430\u044e\u0447\u0438 \u043c\u0456\u043a\u043e\u0431\u0430\u043a\u0442\u0435\u0440\u0456\u0457 \u0442\u0443\u0431\u0435\u0440\u043a\u0443\u043b\u044c\u043e\u0437\u0443),\u043f\u043e\u043b\u0456\u043c\u0456\u0454\u043b\u0456\u0442\u0443,\u0432\u0456\u0440\u0443\u0441\u0443 \u0410(\u041d1N1),\u0441\u043f\u043e\u0440,\u0433\u0440\u0438\u0431\u0456\u0432 Candida, \u0434\u0435\u0440\u043c\u0430\u0442\u043e\u0444\u0456\u0442\u0456\u0432, \u043f\u043b\u0456\u0441\u043d\u044f\u0432\u0438\u0445 \u0433\u0440\u0438\u0431\u043a\u0456\u0432,\u0432\u0456\u0440\u0443\u0441\u0456\u0432 \u0415\u0421\u041d\u041e, \u041a\u043e\u043a\u0441\u0430\u043a\u0456, \u0412\u0406\u041b(\u0421\u041d\u0406\u0414), \u043f\u043e\u043b\u0456\u043e\u043c\u0456\u0454\u043b\u0456\u0442\u0443.\n4. \u0417\u0430\u0441\u0456\u0431 \u0437\u0440\u0443\u0447\u043d\u0438\u0439 \u043f\u0440\u0438 \u0437\u0430\u0441\u0442\u043e\u0441\u0443\u0432\u0430\u043d\u043d\u0456 (\u0443 \u0432\u0438\u0433\u043b\u044f\u0434\u0456 \u0442\u0430\u0431\u043b\u0435\u0442\u043e\u043a).\n5. \u041c\u043e\u0436\u043b\u0438\u0432\u0456\u0441\u0442\u044c \u043f\u0440\u043e\u0432\u0435\u0434\u0435\u043d\u043d\u044f \u043f\u043e\u0442\u043e\u0447\u043d\u043e\u0457 \u0442\u0430 \u0437\u0430\u043a\u043b\u044e\u0447\u043d\u043e\u0457 \u0434\u0435\u0437\u0456\u043d\u0444\u0435\u043a\u0446\u0456\u0457 \u0432 \u041b\u041f\u0417. \n6. \u041c\u043e\u0436\u043b\u0438\u0432\u0456\u0441\u0442\u044c \u043f\u0440\u043e\u0432\u0435\u0434\u0435\u043d\u043d\u044f \u0434\u0435\u0437\u0456\u043d\u0444\u0435\u043a\u0446\u0456\u0457, \u0441\u0443\u043c\u0456\u0449\u0435\u043d\u043e\u0457 \u0437 \u043f\u0435\u0440\u0435\u0434\u0441\u0442\u0435\u0440\u0438\u043b\u0456\u0437\u0430\u0446\u0456\u0439\u043d\u0438\u043c \u043e\u0447\u0438\u0449\u0435\u043d\u043d\u044f\u043c \u0432\u0438\u0440\u043e\u0431\u0456\u0432 \u043c\u0435\u0434\u0438\u0447\u043d\u043e\u0433\u043e \u043f\u0440\u0438\u0437\u043d\u0430\u0447\u0435\u043d\u043d\u044f \u0437 \u043a\u043e\u0440\u043e\u0437\u0456\u0439\u043d\u043e\u0441\u0442\u0456\u0439\u043a\u043e\u0433\u043e \u043c\u0435\u0442\u0430\u043b\u0443, \u0441\u043a\u043b\u0430, \u043f\u043e\u043b\u0456\u043c\u0435\u0440\u043d\u0438\u0445 \u043c\u0430\u0442\u0435\u0440\u0456\u0430\u043b\u0456\u0432.\n7. \u041c\u043e\u0436\u043b\u0438\u0432\u0456\u0441\u0442\u044c \u0437\u043d\u0435\u0437\u0430\u0440\u0430\u0436\u0435\u043d\u043d\u044f \u043c\u0435\u0434\u0438\u0447\u043d\u0438\u0445 \u0432\u0456\u0434\u0445\u043e\u0434\u0456\u0432 (\u0432\u0438\u0440\u043e\u0431\u0456\u0432 \u043c\u0435\u0434\u0438\u0447\u043d\u043e\u0433\u043e \u043f\u0440\u0438\u0437\u043d\u0430\u0447\u0435\u043d\u043d\u044f \u043e\u0434\u043d\u043e\u0440\u0430\u0437\u043e\u0432\u043e\u0433\u043e \u0432\u0438\u043a\u043e\u0440\u0438\u0441\u0442\u0430\u043d\u043d\u044f, \u0432\u0438\u043a\u043e\u0440\u0438\u0441\u0442\u0430\u043d\u043e\u0433\u043e \u043f\u0435\u0440\u0435\u0432\u2019\u044f\u0437\u0443\u0432\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u043c\u0430\u0442\u0435\u0440\u0456\u0430\u043b\u0443 \u0442\u043e\u0449\u043e) \u043f\u0435\u0440\u0435\u0434 \u0457\u0445 \u0443\u0442\u0438\u043b\u0456\u0437\u0430\u0446\u0456\u0454\u044e. \n8. \u041c\u043e\u0436\u043b\u0438\u0432\u0456\u0441\u0442\u044c \u0437\u043d\u0435\u0437\u0430\u0440\u0430\u0436\u0435\u043d\u043d\u044f \u0431\u0456\u043e\u043b\u043e\u0433\u0456\u0447\u043d\u0438\u0445 \u0432\u0456\u0434\u0445\u043e\u0434\u0456\u0432 (\u0441\u0438\u0440\u043e\u0432\u0430\u0442\u043a\u0430, \u043a\u0440\u043e\u0432, \u0441\u0435\u0447\u0430, \u043c\u043e\u043a\u0440\u043e\u0442\u0438\u043d\u043d\u044f, \u0444\u0435\u043a\u0430\u043b\u0456\u0457 \u0442\u043e\u0449\u043e).\n9. \u041c\u043e\u0436\u043b\u0438\u0432\u0456\u0441\u0442\u044c \u0437\u0430\u0441\u0442\u043e\u0441\u0443\u0432\u0430\u043d\u043d\u044f \u043d\u0435\u0432\u0438\u043a\u043e\u0440\u0438\u0441\u0442\u0430\u043d\u043e\u0433\u043e \u0440\u043e\u0437\u0447\u0438\u043d\u0443 \u043f\u0440\u043e\u0442\u044f\u0433\u043e\u043c 3 \u0434\u0456\u0431. \n10. \u041d\u0430\u044f\u0432\u043d\u0456\u0441\u0442\u044c \u0440\u0435\u0436\u0438\u043c\u0443 \u0434\u043b\u044f \u0437\u043d\u0435\u0437\u0430\u0440\u0430\u0436\u0435\u043d\u043d\u044f \u0432\u0456\u0440\u0443\u0441\u0443 \u0433\u0440\u0438\u043f\u0443 \u0410(\u041d1N1).\n11. \u041d\u0430\u044f\u0432\u043d\u0456\u0441\u0442\u044c \u0440\u0435\u0436\u0438\u043c\u0456\u0432 \u0434\u0435\u0437\u0456\u043d\u0444\u0435\u043a\u0446\u0456\u0457 \u043e\u0431 '\u0454\u043a\u0442\u0456\u0432 \u043f\u0440\u0438 \u043f\u0440\u043e\u0432\u0435\u0434\u0435\u043d\u043d\u0456 \u0433\u0435\u043d\u0435\u0440\u0430\u043b\u044c\u043d\u0438\u0445 \u043f\u0440\u0438\u0431\u0438\u0440\u0430\u043d\u044c \u0443 \u043b\u0456\u043a\u0443\u0432\u0430\u043b\u044c\u043d\u043e-\u043f\u0440\u043e\u0444\u0456\u043b\u0430\u043a\u0442\u0438\u0447\u043d\u043e\u043c\u0443 \u0437\u0430\u043a\u043b\u0430\u0434\u0456.\n12. \u041d\u0430\u044f\u0432\u043d\u0456\u0441\u0442\u044c \u0441\u043a\u043e\u0440\u043e\u0447\u0435\u043d\u043e\u0457 \u0435\u043a\u0441\u043f\u043e\u0437\u0438\u0446\u0456\u0457 (\u043d\u0435 \u0431\u0456\u043b\u044c\u0448\u0435 10 \u0445\u0432.) \u0434\u043b\u044f \u0434\u0435\u0437\u0456\u043d\u0444\u0435\u043a\u0446\u0456\u0457 \u043f\u043e\u0432\u0435\u0440\u0445\u043e\u043d\u044c \u043f\u0440\u0438 \u0432\u0456\u0440\u0443\u0441\u043d\u043e\u0457 \u0435\u0442\u0456\u043e\u043b\u043e\u0433\u0456\u0457 (\u0432\u043a\u043b\u044e\u0447\u0430\u044e\u0447\u0438 \u0432\u0456\u0440\u0443\u0441\u0438 \u0433\u0435\u043f\u0430\u0442\u0438\u0442\u0456\u0432, \u0432\u0456\u0440\u0443\u0441 \u0412\u0406\u041b, \u0442\u0430 \u0432\u0456\u0440\u0443\u0441\u0443 \u0433\u0440\u0438\u043f\u0443 \u0410 (H1N1).\n13. \u041a\u0456\u043b\u044c\u043a\u0456\u0441\u0442\u044c \u043b\u0456\u0442\u0440\u0456\u0432 \u0440\u043e\u0431\u043e\u0447\u043e\u0433\u043e \u0440\u043e\u0437\u0447\u0438\u043d\u0443 \u0437 1 \u043a\u0433. \u043a\u043e\u043d\u0446\u0435\u043d\u0442\u0440\u0430\u0442\u0443 \u043f\u0440\u0438 \u043e\u0431\u0440\u043e\u0431\u0446\u0456 \u043f\u043e\u0432\u0435\u0440\u0445\u043e\u043d\u044c \u043f\u0440\u0438 \u0432\u0456\u0440\u0443\u0441\u043d\u0438\u0445 \u0456\u043d\u0444\u0435\u043a\u0446\u0456\u044f\u0445 \u043d\u0435 \u043c\u0435\u043d\u0448\u0435 3333 \u043b. \u043f\u0440\u0438 \u0435\u043a\u0441\u043f\u043e\u0437\u0438\u0446\u0456\u0457 60 \u0445\u0432.\n14. \u0417\u0430\u0441\u0456\u0431 \u043f\u043e\u0432\u0438\u043d\u0435\u043d \u0431\u0443\u0442\u0438 \u0440\u043e\u0437\u0444\u0430\u0441\u043e\u0432\u0430\u043d\u0438\u0439  \u043f\u043e 1\u043a\u0433.\n\n1.\t\u041b\u0438\u0441\u0442 \u0432\u0456\u0434 \u0432\u0438\u0440\u043e\u0431\u043d\u0438\u043a\u0430 \u043d\u0430 \u0456\u043c'\u044f \u0437\u0430\u043c\u043e\u0432\u043d\u0438\u043a\u0430 \u0437 \u0433\u0430\u0440\u0430\u043d\u0442\u0456\u0454\u044e \u0437\u0430\u0431\u0435\u0437\u043f\u0435\u0447\u0435\u043d\u043d\u044f \u0432 \u043f\u043e\u0432\u043d\u043e\u043c\u0443 \u043e\u0431\u0441\u044f\u0437\u0456 \u043f\u043e\u0441\u0442\u0430\u0432\u043a\u0438 \u0434\u0435\u0437\u0456\u043d\u0444\u0435\u043a\u0446\u0456\u0439\u043d\u043e\u0433\u043e \u0437\u0430\u0441\u043e\u0431\u0443.\n2.\t\u041d\u0430 \u043d\u0430\u0434\u0430\u043d\u0443 \u043f\u0440\u043e\u0434\u0443\u043a\u0446\u0456\u044e \u043c\u0430\u0454 \u0431\u0443\u0442\u0438 \u043d\u0430\u0434\u0430\u043d\u043e:\n-\t\u0441\u0435\u0440\u0442\u0438\u0444\u0456\u043a\u0430\u0442 \u044f\u043a\u043e\u0441\u0442\u0456\n-\t\u0441\u0432\u0456\u0434\u043e\u0446\u0442\u0432\u043e \u043f\u0440\u043e \u0434\u0435\u0440\u0436\u0430\u0432\u043d\u0443 \u0440\u0435\u0454\u0441\u0442\u0440\u0430\u0446\u0456\u044e \u043f\u043e\u0441\u0442\u0430\u0447\u0430\u043b\u044c\u043d\u0438\u043a\u0430\n-\t\u0432\u0438\u0441\u043d\u043e\u0432\u043e\u043a \u0434\u0435\u0440\u0436\u0430\u0432\u043d\u043e\u0457 \u0441\u0430\u043d\u0456\u0442\u0430\u0440\u043d\u043e-\u0435\u043f\u0456\u0434\u0435\u043c\u0456\u043e\u043b\u043e\u0433\u0456\u0447\u043d\u043e\u0456 \u044